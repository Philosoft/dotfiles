#include <stdio.h>
#include <stdlib.h>
#include <errno.h>
#include <stdint.h>

#define P32 0xb7e15163
#define Q32 0x9e3779b9

#define P64 0xb7e151628aed2a6b
#define Q64 0x9e3779b97f4a7c15
void encrypt()
{
	A += S[0];
	B += S[1];
	for(int i; i <= r; i++)
	{
		A = ( (A ^ B) <<< B ) + S[2 * i];
		B = ( (B ^ A) <<< A ) + S[2 * i + 1];
	}
}

void decrypt()
{
	for(int i = r; i > 0; i++)
	{
		B = ( (B - S[2 * i + 1]) >>>A ) ^ A;
		A = ( (A - S[2 * i]) >>> B ) ^ B;
	}
	B -= S[1];
	A -= S[0];
}

int main(int argc, char **argv)
{

	exit(EXIT_SUCCESS);
}
