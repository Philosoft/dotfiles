#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char eng[] = " QWERTYUIOP{}ASDFGHJKL:\"ZXCVBNM<>?~`qwertyuiop[]asdfghjkl;'zxcvbnm,./";
char ru[]  = " ЙЦУКЕНГШЩЗХЪФЫВАПРОЛДЖЭЯЧСМИТЬБЮ,Ёёйцукенгшщзхъфывапролджэячсмитьбю.";

#define MAXLEN 69 

int
main(int argc, char *argv[]){
    if(argc != 2){
        printf("USAGE: %s \"str\"", argv[0]);
        exit(0);
    }

    int len = strlen(argv[1]);
    char *res = (char*)malloc(len * sizeof(char));
    memset(res, 'a', len);
    int i, j;
    for(i = 0; i < len; i++){
        for(j = 0; j < MAXLEN; j++){
            if((unsigned char)eng[j] == (unsigned char)argv[1][i]){
                memcpy(res + i, &ru[j], 1);
                break;
            }
        }
    }
    printf("%s\n", res);
    exit(0);
}
