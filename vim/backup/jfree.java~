import org.jfree.chart.ChartFactory;
import org.jfree.chart.ChartFrame;
import org.jfree.chart.JFreeChart;
import org.jfree.data.general.DefaultPieDataset;

public class jfree{
    public static void main(String[] args){
        DefaultPieDataset dataset = new DefaultPieDataset();
        dataset.setValue("Cat 1", 43.2);
        dataset.setValue("Cat 2", 27.9);
        dataset.setValue("Cat 3", 79.5);

        JFreeChart chart = ChartFactory.createPieChart("Sample", dataset, true, true, false);

        ChartFrame frame = new ChartFrame("Firest", chart);
        frame.pack();
        frame.setVisible(true);
    }
}
