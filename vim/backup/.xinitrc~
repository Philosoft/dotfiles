# xrandr --output VGA1 --mode 1600x900 --right-of HDMI1
# xrandr --output HDMI1 --mode 1920x1080 --left-of VGA1
setxkbmap -layout "us,ru" -option "grp:toggle,ctrl:nocaps,compose:rwin"
xrdb -merge ~/.Xdefaults
xsetroot -cursor_name left_ptr
numlockx on
xbindkeys &
(dropbox start &) &
compton -b
ck-launch-session dbus-launch awesome

