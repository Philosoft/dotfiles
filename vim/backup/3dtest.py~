from mpl_toolkits.mplot3d import axes3d
import matplotlib.pyplot as plt
import numpy as np

fig = plt.figure()
ax = fig.add_subplot(111, projection='3d')

X = []
Y = []
Z = []

for buf in open("/tmp/olol", "r"):
	(x, y, z) = buf[:-1].split(' ')
	X.append(float(x))
	Y.append(float(y))
	Z.append(float(z))

ax.scatter3D(X, Y, Z)
plt.show()
