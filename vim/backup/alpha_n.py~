alpha_n = {
	'0': 1.87,
    '0.25': 1.57,
    '0.5': 1.42,
    '0.75': 1.32,
    '1': 1.25,
    '1.25': 1.19,
    '1.5': 1.15,
    '1.75': 1.11,
    '2': 1.08,
    '2.25': 1.05,
    '2.5': 1.02,
    '2.75': 1.00,
    '3': 0.98,
    '3.25': 0.96,
    '3.5': 0.95,
    '3.75': 0.93,
    '4': 0.92,
    '4.25': 0.90,
    '4.5': 0.89,
    '4.75': 0.88,
    '5': 0.87,
    '5.25': 0.86,
    '5.5': 0.86,
    '6': 0.83
}

n = 2.41346375878
n = round(n, 2)
a = 0.0
try:
    a = alpha_n[str(n)]
except KeyError as e:
    left = ''
    right = ''
    n_list = alpha_n.keys()
    n_list.sort()
    for i in n_list:
        if n > float(i):
            left = i
        else:
            right = i
            break
    print 'left =', left, 'right =', right
    delta_left = abs(float(left) - n)
    delta_right =  abs(float(right) - n)
    print 'delta_l =', delta_left, 'delta_right =', delta_right
    if delta_left < delta_right:
        print 'choose left', left
        a = alpha_n[left]
    else:
        print 'right one', right
        a = alpha_n[right]

print 'n =', n, 'a =', a
