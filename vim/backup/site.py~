def compare(reference,number,x,j):
    if x > reference:
        maxdev = deviation[i]
        number = j
#funkciya sravneniya

import random
#import modulya

min = 1
max = 3
n = 5
mean = 0
referance = 10
#usloviya
value = range (0,n)
for i in range(0,n):
    value[i] = random.uniform(min, max)
ok = 0
#Generirovanie

while ok == 0:
    
    for i in range(0,n):
        mean = mean + (value[i] / n)
#srednee
    sum = 0
    deviation = range (0,n)
    for i in range(0,n):
        k = (value[i] - mean)
        print k
        deviation[i] = pow(k, 2)
        sum = sum + deviation[i]
#Otklonenie
    rsd = 100 * ((sum / (n - 1)) ** 0.5 ) / mean
#RSD
    if rsd > referance:
    #Proverka
        if min > max:
            i = min
            min = max
            max = i
        valmin = min - 1
        valmax = max + 1
        maxdev = -1
        extreme = -1
        for i in range(0,n):
            if value[i] < valmin:
                valmin = value[i]
                compare(maxdev,extreme,deviation[i],i)
            if value[i] > valmax:
                valmax = value[i]
                compare(maxdev,extreme,deviation[i],i)
        if value[extreme] > mean:
            i = ((mean + valmax) /2)
            value[extreme] = random.uniform(valmin, i)
        else:
            i = ((mean + valmin) /2)
            value[extreme] = random.uniform(i, valmax)
    else:
        ok = 1
#Podgon

for i in range(0,n):
    print(value[i])
