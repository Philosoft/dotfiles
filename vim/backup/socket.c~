#include <stdio.h>
#include <stdlib.h>
#include <sys/types.h>
#include <sys/socket.h>
#include <errno.h>

int
main(int argc, char **argv)
{
    int sock;
    
    sock = socket(AF_INET, SOCK_STREAM, 6);
    if(sock == -1)
    {
        perror("Something happend\n");
        exit(EXIT_FAILURE);
    }

    printf("socket: %i\n", sock);

    exit(EXIT_SUCCESS);
}
