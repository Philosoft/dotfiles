#include <stdio.h>

int main()
{
	int a, b, c, d;
	printf("Введите a: ");
	scanf("%i", &a);
	printf("Введите b: ");
	scanf("%i", &b);
	printf("Введите c: ");
	scanf("%i", &c);
	printf("Введите d: ");
	scanf("%i", &d);

	double desc = 0;
	desc = -4 * b*b*b*d + b*b * c*c - 4*a * c*c*c + 18*a*b*c*d - 27 * a*a * d*d;
	if(desc >= 0)
	{
		double p, q, Q;
		p = - b*b / 3 / a / a + c / a;
		q = 2 * b*b*b / (27 * a*a*a) - b*c / (3 * a*a) + d / a;

		Q = (p / 3) * (p / 3) * (p / 3) + (q / 2) * (q / 2);

		double y1, y2, y3;
		double alpha, beta;

		alpha = pow(- q / 2 + sqrt(Q), 1.0/3.0);
		beta = pow(- q / 2 - sqrt(Q), 1.0/3.0);

		double x1, x2;
		x1 = alpha + beta;
		x2 = - (alpha + beta) / 2;
	}
	return 0;
}
