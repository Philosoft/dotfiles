"/usr/bin/xbacklight '+5'"
    XF86KbdBrightnessUp

"/usr/bin/xbacklight '-5'"
    XF86KbdBrightnessDown

"/home/philosoft/bin/pulseaudio-ctl mute"
    XF86AudioMute

"/home/philosoft/bin/pulseaudio-ctl up"
    XF86AudioRaiseVolume

"/home/philosoft/bin/pulseaudio-ctl down"
    XF86AudioLowerVolume

"player_ctrl.sh toggle"
    XF86AudioPlay

"xcalc"
    XF86Calculator

"player_ctrl.sh next"
    XF86AudioNext

"player_ctrl.sh prev"
    XF86AudioPrev
