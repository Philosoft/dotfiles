<?php
$passwd = " ";

function inc_char(&$str, $ch)
{
    if(ord($str[$ch]) == 126)
    {
        if($ch > 0)
        {
            $str[$ch] = " ";
            inc_char($str, $ch - 1);
            return;
        }
        else
        {
            $str .= " ";
            $rule = strlen($str) - 1;
            for($i = 0; $i < $rule; $i++)
                $str[$i] = " ";

            return;
        }
    }

    $str[$ch] = chr( ord($str[$ch]) + 1 );
}

$f = fopen("passwds.dat", "w") or die("Вот чесслово хуита какая-то");

while(strlen($passwd) != 4)
{
    echo "Trying passwd: $passwd\n";
    fwrite($f, $passwd . "\n")
    inc_char($passwd, strlen($passwd) - 1);
}
?>
