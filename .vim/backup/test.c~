#include <stdio.h>
#include <stdlib.h>

typedef struct
{
    char name[50];
    int grade;
} Student;

typedef Student (*StudentMatType)[3];

int main(int argc, char** argv)
{
    printf("1. 0x%x\n2. 0x%x\n3. 0x%x\n", sizeof(StudentMatType), sizeof(int*), sizeof(char*));
    StudentMatType p;
    p = (StudentMatType)malloc(50 * 4 * 3);
    p[0] -> grade = 0, p[1] -> grade = 1, p[2] -> grade = 2;
    printf("%d, %d, %d\n%d\n%x\n", p[0] -> grade, p[1] -> grade, p[2] -> grade, sizeof(p), sizeof(*p));
    return 0;
}
